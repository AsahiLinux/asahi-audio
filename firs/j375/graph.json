{
    "node.description": "Mac Studio Speaker",
    "media.name": "Mac Studio Speaker",
    "filter.graph": {
        "nodes": [
            {
                "type": "lv2",
                "plugin": "https://chadmed.au/bankstown",
                "name": "bassex",
                "control": {
                    "bypass": 0,
                    "amt": 1.45,
                    "sat_second": 1.75,
                    "sat_third": 2.35,
                    "blend": 0.65,
                    "ceil": 200.0,
                    "floor": 20.0,
                }
            },
            {
                "type": "lv2",
                "plugin": "http://lsp-plug.in/plugins/lv2/loud_comp_mono",
                "name": "ell",
                "control": {
                    "enabled": 1,
                    "input": 1.0,
                    "fft": 4
                }
            },
            {
                "type": "lv2",
                "plugin": "http://lsp-plug.in/plugins/lv2/loud_comp_mono",
                "name": "elr",
                "control": {
                    "enabled": 1,
                    "input": 1.0,
                    "fft": 4
                }
            },
            {
                "type": "builtin",
                "label": "convolver",
                "name": "convL",
                "config": {
                    "filename": [
                        "/usr/share/asahi-audio/j375/44.wav",
                        "/usr/share/asahi-audio/j375/48.wav",
                        "/usr/share/asahi-audio/j375/96.wav",
                        "/usr/share/asahi-audio/j375/192.wav"
                    ],
                    "channel": 0
                }
            },
            {
                "type": "builtin",
                "label": "convolver",
                "name": "convR",
                "config": {
                    "filename": [
                        "/usr/share/asahi-audio/j375/44.wav",
                        "/usr/share/asahi-audio/j375/48.wav",
                        "/usr/share/asahi-audio/j375/96.wav",
                        "/usr/share/asahi-audio/j375/192.wav"
                    ],
                    "channel": 0
                }
            },
            {
                "type": "lv2",
                "plugin": "http://lsp-plug.in/plugins/lv2/compressor_stereo",
                "name": "woofer_lim",
                "control": {
                    "sla": 5.0,
                    "al": 1.0,
                    "at": 1.0,
                    "rt": 100.0,
                    "cr": 15.0,
                    "kn": 0.5
                }
            }
        ],
        "links": [
            {"output": "bassex:out_l", "input": "ell:in"},
            {"output": "bassex:out_r", "input": "elr:in"},
            {"output": "ell:out", "input": "convL:In"},
            {"output": "elr:out", "input": "convR:In"},
            {"output": "convL:Out", "input": "woofer_lim:in_l"},
            {"output": "convR:Out", "input": "woofer_lim:in_r"},
        ],
        "inputs": [
            "bassex:in_l",
            "bassex:in_r"
        ],
        "outputs": [
            "woofer_lim:out_l",
            "woofer_lim:out_r"
        ],
        "capture.volumes": [
            {
                "control": "ell:volume",
                "min": -40.0,
                "max": 0.0,
                "scale": "cubic"
            },
            {
                "control": "elr:volume",
                "min": -40.0,
                "max": 0.0,
                "scale": "cubic"
            }
        ]
    },
    "capture.props": {
        "node.name": "audio_effect.studio-convolver",
        "media.class": "Audio/Sink",
        "audio.channels": "2",
        "audio.position": ["FL", "FR"],
        "audio.rate": 48000,
        "node.virtual": "false",
        "priority.session": 850,
        "state.default-channel-volume": 0.343,
    },
    "playback.props": {
        "node.name": "effect_output.j375-convolver",
        "target.object": "alsa_output.platform-sound.HiFi__hw_AppleJ375_1__sink",
        "node.passive": "true",
        "audio.channels": "2",
        "audio.rate": 48000,
        "audio.position": ["FL", "FR"]
    }
}
